	langid = {english},
	file = {Weisfeld et al. - 2003 - Possible olfaction-based mechanisms in human kin r.pdf:/Users/andreas/Zotero/storage/IHVBTP5X/Weisfeld et al. - 2003 - Possible olfaction-based mechanisms in human kin r.pdf:application/pdf},
}

