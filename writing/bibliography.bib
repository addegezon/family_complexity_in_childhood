	file = {Buskell et al. - 2019 - A systems approach to cultural evolution.pdf:/Users/andreas/Zotero/storage/WENF4V7K/Buskell et al. - 2019 - A systems approach to cultural evolution.pdf:application/pdf},
}

@article{heyes_precis_2019,
