@article{lindstrom_short-_2002,
	title = {The Short- and Long-Term Effects of U.S. Migration Experience on Mexican Women's Fertility},
	volume = {80},
	issn = {00377732},
	url = {https://ezp.sub.su.se/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=aph&AN=6767698&site=ehost-live&scope=site},
	doi = {10.1353/sof.2002.0030},
	abstract = {Using retrospective fertility and migration histories from a binational study of Mexico-U.S. migration, we test for the presence of separation, assimilation, adaptation, diffusion, and selectivity effects of migration on annual birth probabilities and completed fertility. Our results reveal that spousal separation due to temporary migration reduces birth probabilities in the short term but does not reduce marital fertility in the long term. However, when women migrate to the U.S. either as temporary migrants or as long-term settled migrants, their experiences lead to lower birth probabilities while in the U.S. as well as to fewer total births. By contrast, U.S. migration experience among men who return to Mexico is associated with higher marital fertility in Mexico, suggesting that temporary migrants are selected for higher fertility.},
	pages = {1341},
	number = {4},
	journaltitle = {Social Forces},
	shortjournal = {Social Forces},
	author = {Lindstrom, David P. and Saucedo, Silvia Giorguli},
