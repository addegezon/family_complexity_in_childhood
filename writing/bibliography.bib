	note = {Publisher: Royal Society},
	file = {JSTOR Full Text PDF:/Users/andreas/Zotero/storage/QSDLCDPM/Heyes - 2012 - Simple minds a qualified defence of associative l.pdf:application/pdf},
}

@article{lind_dating_2013,
